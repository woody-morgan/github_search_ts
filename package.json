{
  "name": "green_labs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn relay --watch\" \"next dev\"",
    "build": "yarn relay && next build",
    "start": "yarn relay && next start",
    "prepare": "husky install",
    "relay": "relay-compiler",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix --ext .js,.jsx,.ts,.tsx ."
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "graphql": "^16.6.0",
    "immer": "^9.0.16",
    "lodash-es": "^4.17.21",
    "next": "^12.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-relay": "10.0.0",
    "relay-runtime": "10.0.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "18.11.3",
    "@types/react": "18.0.21",
    "@types/react-relay": "^14.1.2",
    "@types/relay-runtime": "^14.1.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "autoprefixer": "^10.4.12",
    "babel-plugin-relay": "10.0.0",
    "concurrently": "^7.5.0",
    "eslint": "^8.25.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sonarjs": "^0.16.0",
    "husky": ">=7",
    "lint-staged": ">=10",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "relay-compiler": "^14.1.0",
    "tailwindcss": "^3.2.0",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ],
    "src/**": [
      "prettier --write ."
    ]
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "not ie <= 11"
  ]
}
